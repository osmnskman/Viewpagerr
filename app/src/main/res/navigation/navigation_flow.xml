<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_flow"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/onboardingAllFragment3"
        android:name="com.osmansekman.andorid_viewpager_osmnskman.fragment.onboardingAllFragment"
        android:label="onboardingAllFragment" >
        <action
            android:id="@+id/action_onboardingAllFragment3_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.osmansekman.andorid_viewpager_osmnskman.fragment.splashFragment"
        android:label="splashFragment" >
        <action
            android:id="@+id/action_splashFragment_to_onboardingAllFragment3"
            app:destination="@id/onboardingAllFragment3" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.osmansekman.andorid_viewpager_osmnskman.fragment.homeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
</navigation>